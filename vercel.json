{
    "version": 2,
    "builds": [
        {
            "src": "pyapi/**/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "nuxt/**/api/*",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/pyapi/(.*)",
            "dest": "/pyapi/$1.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "/nuxt/api/$1"
        }
    ]
}